{
  "name": "archive-35-studio",
  "version": "0.1.0",
  "description": "Content management and automation hub for Archive-35 photography",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run react-dev\" \"wait-on http://localhost:3000 && electron .\"",
    "react-dev": "react-scripts start",
    "build": "react-scripts build && electron-builder",
    "test": "react-scripts test"
  },
  "author": "Wolfgang Schram",
  "license": "UNLICENSED",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "electron-is-dev": "^2.0.0",
    "@anthropic-ai/sdk": "^0.24.0",
    "@aws-sdk/client-s3": "^3.500.0",
    "axios": "^1.6.0",
    "exiftool-vendored": "^24.0.0",
    "sharp": "^0.33.0",
    "dotenv": "^16.3.0"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.0",
    "concurrently": "^8.2.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.archive35.studio",
    "productName": "Archive-35 Studio",
    "mac": {
      "category": "public.app-category.photography",
      "icon": "assets/icon.icns"
    },
    "files": [
      "build/**/*",
      "main.js",
      "preload.js"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  }
}
